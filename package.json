{
    "name": "@spyeic/dplayer",
    "version": "1.0.0",
    "description": "Wow, such a lovely HTML5 danmaku video player",
    "main": "dist/DPlayer.min.js",
    "types": "types/index.d.ts",
    "scripts": {
        "start": "npm run dev",
        "build": "cross-env NODE_ENV=production webpack --config webpack/prod.config.js --progress && tsc",
        "dev": "cross-env NODE_ENV=development webpack serve --config webpack/dev.config.js",
        "test": "",
        "format": "prettier \"**/*.{js,json,md}\" --write",
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "prepare": "husky install"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "url": "git+https://github.com/spyeic/DPlayer.git",
        "type": "git"
    },
    "keywords": [
        "player",
        "danmaku",
        "video",
        "html5"
    ],
    "author": "spyeic",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/spyeic/DPlayer/issues"
    },
    "homepage": "https://github.com/spyeic/DPlayer#readme",
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/preset-env": "^7.25.3",
        "@vuepress/plugin-pwa": "1.9.9",
        "art-template": "4.13.2",
        "art-template-loader": "1.4.3",
        "autoprefixer": "^10.4.19",
        "babel-loader": "^9.1.3",
        "cross-env": "^7.0.3",
        "css-loader": "^6.11.0",
        "cssnano": "^6.1.2",
        "exports-loader": "^4.0.0",
        "file-loader": "^6.2.0",
        "git-revision-webpack-plugin": "^5.0.0",
        "husky": "^8.0.3",
        "less": "^4.2.0",
        "less-loader": "^11.1.4",
        "lint-staged": "^13.3.0",
        "mini-css-extract-plugin": "2.7.6",
        "postcss": "^8.4.40",
        "postcss-loader": "^7.3.4",
        "postcss-preset-env": "^9.6.0",
        "prettier": "^3.3.3",
        "prettier-check": "^2.0.0",
        "pretty-quick": "^4.0.0",
        "style-loader": "^3.3.4",
        "svg-inline-loader": "0.8.2",
        "template-string-optimize-loader": "^3.0.0",
        "typescript": "^5.5.4",
        "url-loader": "^4.1.1",
        "webpack": "^5.93.0",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "^4.15.2",
        "yorkie": "^2.0.0"
    },
    "dependencies": {
        "axios": "1.6.0",
        "balloon-css": "^1.2.0",
        "promise-polyfill": "8.3.0"
    }
}
